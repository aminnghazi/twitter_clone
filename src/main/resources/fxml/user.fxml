<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<GridPane fx:id="gridPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="120.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="80.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
            <children>
                <HBox>
                    <children>
                        <Label fx:id="name" styleClass="label-big-bold" text="name">
                            <HBox.margin>
                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="id" styleClass="label-normal" text="id" textFill="#7a7676">
                            <HBox.margin>
                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                            </HBox.margin>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets left="10.0" top="10.0" />
                    </VBox.margin>
                </HBox>
            <VBox fx:id="customItem" prefHeight="200.0" prefWidth="100.0">
               <padding>
                  <Insets left="20.0" top="10.0" />
               </padding>
            </VBox>
            </children>
        </VBox>
        <Circle fx:id="profilePic" fill="DODGERBLUE" radius="30.0" stroke="TRANSPARENT" strokeType="INSIDE">
         <GridPane.margin>
            <Insets bottom="10.0" />
         </GridPane.margin></Circle>
    </children>
    <stylesheets>
        <URL value="@../CSS/custom.css" />
    </stylesheets>
</GridPane>
